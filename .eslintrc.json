{
  "env": {
    "shared-node-browser": true,
    "es2022": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn",
    "import/extensions": "off",
    "no-unused-vars": "warn",
    "arrow-body-style": "warn",
    "no-var": "warn"
  }
}
